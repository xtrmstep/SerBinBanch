namespace FlatBuffersSample;

table Message {
    people:[Person];
}

table Person {
	_id:string;
	index:string;
	guid:string;
	isActive:bool;
	balance:float;
	picture:string;
	age:uint;
	eyeColor:string;
	name:string;
	gender:string;
	company:string;
	email:string;
	phone:string;
	address:string;
	about:string;
	registered:string;
	latitude:float;
	longitude:float;
	tags:[string];
	friends:[Friend];
}

table Friend {
    id:string;
    name:string;
}

root_type Message;
